{
	"compilerOptions": {
		"baseUrl": ".",
		"module": "commonjs",
		"target": "es6",
		"jsx": "react",
		"checkJs": true,
		"paths": {
			"assets/*": ["src/assets/*"],
			"components/*": ["src/components/*"],
			"pages/*": ["src/pages/*"],
		}
	},
	"exclude": ["./node_modules/*", "./dist/*"],
	"include": ["src/**/*"]
}
