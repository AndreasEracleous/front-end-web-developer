{
	"name": "Andreas-Eracleous-website",
	"private": true,
	"description": "Andreas Eracleous website developing with Gatsby",
	"version": "0.1.0",
	"author": "Andreas Eracleous <andrew.eracleous@gmail.com>",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.34",
		"@fortawesome/free-brands-svg-icons": "^5.15.2",
		"@fortawesome/free-solid-svg-icons": "^5.15.2",
		"@fortawesome/react-fontawesome": "^0.1.14",
		"autoprefixer": "^10.1.0",
		"bootstrap-scss": "4.6.0",
		"cssnano": "^4.1.10",
		"disqus-react": "^1.0.7",
		"dotenv": "^8.2.0",
		"gatsby": "^2.31.1",
		"gatsby-cli": "^2.18.0",
		"gatsby-image": "^2.2.31",
		"gatsby-plugin-breadcrumb": "^10.0.0",
		"gatsby-plugin-feed": "^2.3.25",
		"gatsby-plugin-layout": "^1.9.0",
		"gatsby-plugin-manifest": "^2.2.27",
		"gatsby-plugin-netlify": "^2.3.11",
		"gatsby-plugin-offline": "^3.0.30",
		"gatsby-plugin-preact": "^4.4.0",
		"gatsby-plugin-preconnect": "^1.1.54",
		"gatsby-plugin-purgecss": "^5.0.0",
		"gatsby-plugin-react-helmet": "^3.1.16",
		"gatsby-plugin-react-svg": "^3.0.0",
		"gatsby-plugin-robots-txt": "^1.5.1",
		"gatsby-plugin-sass": "^2.1.23",
		"gatsby-plugin-sharp": "^2.11.2",
		"gatsby-plugin-sitemap": "^2.2.22",
		"gatsby-source-filesystem": "^2.1.40",
		"gatsby-transformer-remark": "^2.6.39",
		"gatsby-transformer-sharp": "^2.3.3",
		"node-sass": "^4.14.1",
		"preact": "^10.5.7",
		"preact-render-to-string": "^5.1.12",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.1.0",
		"stylelint": "^13.8.0"
	},
	"devDependencies": {
		"@testing-library/react": "^11.1.0",
		"@testing-library/react-hooks": "^3.3.0",
		"@testing-library/user-event": "^12.1.10",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.16.0",
		"eslint-config-airbnb": "18.1.0",
		"eslint-config-node": "^4.0.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "2.5.0",
		"eslint-plugin-security": "^1.4.0",
		"gatsby-plugin-eslint": "^2.0.8",
		"gatsby-plugin-root-import": "^2.0.5",
		"husky": "^4.2.5",
		"identity-obj-proxy": "^3.0.0",
		"lint-staged": "^10.2.7",
		"prettier": "^2.0.5",
		"react-test-renderer": "^16.13.1",
		"stylelint-config-standard": "^20.0.0"
	},
	"engines": {
		"node": ">=12",
		"yarn": ">=1.22"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,jsx}": "yarn run lint:js --cache",
		"src/**/*.{css}": "yarn run lint:css"
	},
	"peerDependencies": {
		"eslint-config-airbnb": "^18.1.0"
	},
	"browser": {
		"electron": false
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"scripts": {
		"build": "yarn run clean && gatsby build",
		"develop": "gatsby develop",
		"lint": "yarn run lint:js && yarn run lint:css",
		"lint:js": "eslint --fix ./src --ext .js,.jsx",
		"lint:css": "stylelint \"src/**/*.css\" --fix && prettier \"src/**/*.css\" --write",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"test": "TZ=UTC jest"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gatsbyjs/gatsby-starter-default"
	},
	"bugs": {
		"url": "https://github.com/gatsbyjs/gatsby/issues"
	}
}
